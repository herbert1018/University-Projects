        "id": "4b9e5aee144fed9f",
        "type": "function",
        "z": "54eb744b6df87ea8",
        "name": "處理人流圖表資料",
        "func": "// 建立符合Chart格式的資料結構\nconst result = [{\n    \"series\": [\"人流量\", \"進入\", \"離開\"],\n    \"data\": [[], [], []],\n    \"labels\": [\"\"]\n}];\n\n// 處理每筆記錄\nfor (let item of msg.payload) {\n    var timestamp = item.timestamp;\n    var count = item.count;\n    var inCount = item.in_count;\n    var outCount = item.out_count;\n    \n    result[0].data[0].push({ \"x\": timestamp, \"y\": count });\n    result[0].data[1].push({ \"x\": timestamp, \"y\": inCount });\n    result[0].data[2].push({ \"x\": timestamp, \"y\": outCount });\n}\n\nmsg.payload = result;\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 780,
        "y": 60,
