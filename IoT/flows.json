            ]
        ]
    },
    {
        "id": "1ec7245730f46428",
        "type": "function",
        "z": "54eb744b6df87ea8",
        "name": "處理強闖清單資料",
        "func": "// 建立HTML清單格式\nvar htmlContent = '<div style=\"font-family: Arial, sans-serif;\">';\nhtmlContent += '<h4 style=\"color: #d32f2f; margin-bottom: 10px;\">🚨 強闖記錄</h4>';\n\nif (msg.payload && msg.payload.length > 0) {\n    htmlContent += '<div style=\"max-height: 300px; overflow-y: auto;\">';\n    \n    for (let item of msg.payload) {\n        var timeStr = new Date(item.timestamp).toLocaleString('zh-TW');\n        var status = item.alert ? '⚠️ 強闖警報' : '✅ 正常';\n        var bgColor = item.alert ? '#ffebee' : '#e8f5e8';\n        var borderColor = item.alert ? '#f44336' : '#4caf50';\n        \n        htmlContent += '<div style=\"';\n        htmlContent += 'margin: 5px 0; padding: 10px; ';\n        htmlContent += 'background-color: ' + bgColor + '; ';\n        htmlContent += 'border-left: 3px solid ' + borderColor + '; ';\n        htmlContent += 'border-radius: 4px; font-size: 14px;\">';\n        htmlContent += '<div><strong>時間：</strong>' + timeStr + '</div>';\n        htmlContent += '<div><strong>位置：</strong>' + item.location + '</div>';\n        htmlContent += '<div><strong>狀態：</strong>' + status + '</div>';\n        htmlContent += '</div>';\n    }\n    \n    htmlContent += '</div>';\n} else {\n    htmlContent += '<p style=\"color: #666; text-align: center; padding: 20px;\">暫無強闖記錄</p>';\n}\n\nhtmlContent += '</div>';\n\nmsg.payload = htmlContent;\nreturn msg;",
